    <command:returnValues>
      <command:returnValue>
        <dev:type>
          <maml:name>PSShareFileClient</maml:name>
          <maml:uri />
          <maml:description>
            <maml:para>
              A ShareFile API client; to be used by the other commands and providers in this SnapIn.
            </maml:para>
          </maml:description>
        </dev:type>
      </command:returnValue>
    </command:returnValues>

