    <command:returnValues>
      <command:returnValue>
        <dev:type>
          <maml:name>ShareFile.Api.Models.ODataObject</maml:name>
          <maml:uri />
          <maml:description>
            <maml:para>
              All API responses are deserialized into ShareFile.Api.Model classes, sub-types of ShareFile.Api.Models.ODataObject.
            </maml:para>
          </maml:description>
        </dev:type>
      </command:returnValue>
